{
  "name": "brainly_tools",
  "version": "6.0.0",
  "description": "A toolbox for Brainly markets. It helps moderators for doing some quick actions and more..",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "chrome-build": "cross-env TARGET=chrome gulp",
    "opera-build": "cross-env TARGET=opera gulp",
    "firefox-build": "cross-env TARGET=firefox gulp",
    "build": "cross-env NODE_ENV=production npm run chrome-build && cross-env NODE_ENV=production npm run opera-build && cross-env NODE_ENV=production npm run firefox-build",
    "chrome-watch": "cross-env TARGET=chrome gulp watch",
    "opera-watch": "cross-env TARGET=opera gulp watch",
    "firefox-watch": "cross-env TARGET=firefox gulp watch",
    "chrome-dist": "cross-env NODE_ENV=production cross-env TARGET=chrome gulp dist",
    "opera-dist": "cross-env NODE_ENV=production cross-env TARGET=opera gulp dist",
    "firefox-dist": "cross-env NODE_ENV=production cross-env TARGET=firefox gulp dist",
    "dist": "npm run chrome-dist && npm run opera-dist && npm run firefox-dist"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/seahindeniz/BrainlyTools_Extension"
  },
  "author": "Sahin D. <seahindeniz@gmail.com> (https://gitlab.com/seahindeniz)",
  "bugs": {
    "url": "https://gitlab.com/seahindeniz/BrainlyTools_Extension/issues"
  },
  "homepage": "https://gitlab.com/seahindeniz/BrainlyTools_Extension",
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "babel-plugin-add-module-exports": "^0.3.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-latest": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^3.2.4",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-bro": "^1.0.3",
    "gulp-clean": "^0.3.1",
    "gulp-eslint": "^2.0.0",
    "gulp-if": "^2.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^0.5.3",
    "gulp-merge-json": "^1.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "*",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-zip": "^2.0.3",
    "preprocessify": "^1.0.1",
    "uglifyify": "^5.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "easyeditor": "^1.2.0",
    "js-cookie": "^2.2.0",
    "js-md5": "^0.7.3",
    "js-yaml": "^3.12.0",
    "moment": "^2.22.2"
  }
}
